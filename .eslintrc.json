{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "shared-node-browser": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-param-reassign": 0,
    "no-restricted-globals": 0,
    "linebreak-style": 0,
    "import/prefer-default-export": 0
  }
}
